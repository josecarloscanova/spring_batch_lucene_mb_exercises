package org.nanotek.base;

@SuppressWarnings("serial")
public class StrategyExcetion extends RuntimeException {

	public StrategyExcetion() {
	}

	public StrategyExcetion(String message) {
		super(message);
	}

	public StrategyExcetion(Throwable cause) {
		super(cause);
	}

	public StrategyExcetion(String message, Throwable cause) {
		super(message, cause);
	}

	public StrategyExcetion(String message, Throwable cause,
			boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
	}

}
