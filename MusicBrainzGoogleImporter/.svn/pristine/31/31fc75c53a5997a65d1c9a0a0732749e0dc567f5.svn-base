package org.nanotek.lucene;

@SuppressWarnings("serial")
public class TemplateBuilderException extends RuntimeException {

	public TemplateBuilderException() {
		super();
	}

	public TemplateBuilderException(String message, Throwable cause,
			boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
	}

	public TemplateBuilderException(String message, Throwable cause) {
		super(message, cause);
	}

	public TemplateBuilderException(String message) {
		super(message);
	}

	public TemplateBuilderException(Throwable cause) {
		super(cause);
	}

}
