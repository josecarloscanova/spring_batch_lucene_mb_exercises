package org.nanotek.lucene.index;

import org.nanotek.Base;

@SuppressWarnings("serial")
public abstract class StopWord implements Base<Long> {

	private Long id; 
	
	public StopWord() {
	}
	
	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public abstract String getWord();
	
}
