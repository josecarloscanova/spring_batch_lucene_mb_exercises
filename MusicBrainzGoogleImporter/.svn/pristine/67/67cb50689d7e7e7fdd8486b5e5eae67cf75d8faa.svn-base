package org.nanotek.tika;

@SuppressWarnings("serial")
public class MediaDetectionException extends Exception {

	public MediaDetectionException() {
	}

	public MediaDetectionException(String message) {
		super(message);
	}

	public MediaDetectionException(Throwable cause) {
		super(cause);
	}

	public MediaDetectionException(String message, Throwable cause) {
		super(message, cause);
	}

	public MediaDetectionException(String message, Throwable cause,
			boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
	}

}
