<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.nanotek.beans">

    <class name="ArtistAlias" table="ARTIST_ALIAS">

    	<id name="id" type="long"  column="ID">
			 <generator class="assigned" />
        </id>
        
		<property name="artistId" type="long"  column="ARTIST_ID"/>
		<property name="name" type="string" column="NAME" />
		<property name="sortName" type="string" column="SORT_NAME" />
        <property name="locale" type="string" column="LOCALE" />
        
    </class>
    
</hibernate-mapping>