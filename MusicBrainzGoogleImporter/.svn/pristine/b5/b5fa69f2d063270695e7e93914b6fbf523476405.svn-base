package org.nanotek.lucene.util;

public class ThreadListenerException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public ThreadListenerException() {
	}

	public ThreadListenerException(String message) {
		super(message);
	}

	public ThreadListenerException(Throwable cause) {
		super(cause);
	}

	public ThreadListenerException(String message, Throwable cause) {
		super(message, cause);
	}

	public ThreadListenerException(String message, Throwable cause,
			boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
	}

}
