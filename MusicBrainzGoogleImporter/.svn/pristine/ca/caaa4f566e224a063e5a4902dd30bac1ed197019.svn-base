<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.nanotek.beans">

    <class name="WorkAlias" table="WORK_ALIAS">
        
    	<id name="id" type="long"  column="ID">
			 <generator class="assigned" />
        </id>
        
        <property name="work" type="long"  column="WORK"/>
        <property name="name" type="long" column="NAME" />
        <property name="locale" type="string" column="LOCALE" />
        
    </class>
    
</hibernate-mapping>