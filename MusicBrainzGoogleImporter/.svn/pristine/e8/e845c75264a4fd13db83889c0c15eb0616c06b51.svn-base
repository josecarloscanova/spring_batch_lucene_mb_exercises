package org.nanotek.beans.mapping;

import org.nanotek.beans.Url;

import au.com.bytecode.opencsv.bean.ColumnPositionMappingStrategy;

public class UrlMapping extends ColumnPositionMappingStrategy<Url>{

	public UrlMapping() 
	{ 
		String [] columns = new String [5]; 
		
		columns[0] = "id";
		columns[1] = "gif";
		columns[2] = "url";
		columns[3] = "description";
		columns[4] = "refCount";
		
		this.setColumnMapping(columns);
	}
}
