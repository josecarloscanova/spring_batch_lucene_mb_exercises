package org.nanotek.lucene.index.info;

import org.apache.lucene.index.Term;
import org.nanotek.base.LongBase;
import org.nanotek.lucene.index.Index;
import org.nanotek.lucene.index.info.terms.TermScope;

@SuppressWarnings("serial")
public class TermStats extends LongBase  {

	private Index index; 
	private Term term;
	private Long totalTermFrequency; 
	private Integer termFrequency; 
	private TermScope termScope; 
	
	public Index getIndex() {
		return index;
	}

	public void setIndex(Index index) {
		this.index = index;
	}

	public Term getTerm() {
		return term;
	}

	public void setTerm(Term term) {
		this.term = term;
	}

	public Long getTotalTermFrequency() {
		return totalTermFrequency;
	}

	public void setTotalTermFrequency(Long totalTermFrequency) {
		this.totalTermFrequency = totalTermFrequency;
	}

	public Integer getTermFrequency() {
		return termFrequency;
	}

	public void setTermFrequency(Integer termFrequency) {
		this.termFrequency = termFrequency;
	}

	public TermScope getTermScope() {
		return termScope;
	}

	public void setTermScope(TermScope termScope) {
		this.termScope = termScope;
	}

}
